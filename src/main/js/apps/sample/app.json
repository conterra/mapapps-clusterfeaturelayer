{
    "properties": {
        "title": "${apptitle}"
    },
    "load": {
        "bundleLocations": [
            "localbundles",
            "bundles"
        ],
        "allowedBundles": [
            "agssearch",
            "banner",
            "basemaptoggler",
            "languagetoggler",
            "locateme",
            "locator-store",
            "map-init",
            "map-preload-2D",
            "mapnavigation",
            "measurement-2d",
            "notifier",
            "parameter-url",
            "popups-default",
            "printing",
            "result-ui",
            "scalebar",
            "search-ui",
            "selection-ui",
            "sketching",
            "splashscreen",
            "system",
            "template-seasons",
            "templatelayout",
            "theme-autumn",
            "toc",
            "toolset",
            "dn_clusterfeaturelayer",
            "dn_imprintprivacy"
        ],
        "i18n": [
            "bundle"
        ]
    },
    "bundles": {
        "dn_clusterfeaturelayer": {
            "ClusterSymbolProvider": {
                "clusterSymbolSize": 25,
                "clusterBackgroundSymbolColor": [
                    200,
                    200,
                    200,
                    0.5
                ],
                "clusterBackgroundBorderColor": [
                    0,
                    0,
                    0,
                    1
                ],
                "clusterBackgroundBorderSize": 0.5,
                "clusterAreaSymbolColor": [
                    255,
                    255,
                    0,
                    0.25
                ],
                "clusterAreaSymbolBorderColor": [
                    0,
                    0,
                    0,
                    1
                ],
                "clusterAreaSymbolBorderSize": 0.5,
                "clusterSingleSymbolColor": [
                    140,
                    177,
                    210,
                    0.75
                ],
                "clusterSingleSymbolBorderColor": [
                    140,
                    177,
                    210,
                    0.35
                ],
                "clusterSingleSymbolBorderSize": 20,
                "spiderfyingLineSize": 0.5,
                "spiderfyingLineColor": [
                    0,
                    0,
                    0,
                    1
                ],
                "spiderfyingCenterColor": [
                    0,
                    0,
                    0,
                    1
                ],
                "spiderfyingCenterSize": 5,
                "labelColor": [
                    0,
                    0,
                    0,
                    1
                ],
                "labelHaloColor": [
                    255,
                    255,
                    255,
                    1
                ],
                "labelFontSize": "10pt",
                "labelHaloSize": 1,
                "labelFontFamily": "Arial"
            }
        },
        "agssearch": {
            "AutoStoreRegistration": {
                "componentEnabled": true
            }
        },
        "banner": {
            "BannerWidget": {
                "label": "Developer Network",
                "image": "resource('${app}:/assets/logo.svg')",
                "imageWidth": 150,
                "imagePosition": "above",
                "link": "http://conterra.github.io"
            }
        },
        "basemaptoggler": {
            "Config": {
                "itemStyles": {
                    "gray": "icon-thumbnail",
                    "streets": "icon-thumbnail",
                    "satellite": "icon-thumbnail",
                    "hybrid": "icon-thumbnail"
                }
            }
        },
        "map-init": {
            "Config": {
                "basemaps": [
                    {
                        "id": "gray",
                        "basemap": "gray-vector",
                        "title": "${map.basemaps.gray}",
                        "thumbnailUrl": "resource('${app}:/assets/gray.png')",
                        "selected": true
                    },
                    {
                        "id": "streets",
                        "basemap": "streets-vector",
                        "title": "${map.basemaps.streets}",
                        "thumbnailUrl": "resource('${app}:/assets/streets.png')"
                    },
                    {
                        "id": "satellite",
                        "basemap": "satellite",
                        "title": "${map.basemaps.topo}",
                        "thumbnailUrl": "resource('${app}:/assets/topo.png')"
                    },
                    {
                        "id": "hybrid",
                        "basemap": "hybrid",
                        "title": "${map.basemaps.hybrid}",
                        "thumbnailUrl": "resource('${app}:/assets/hybrid.png')"
                    }
                ],
                "map": {
                    "layers": [
                        {
                            "id": "clusterfeaturelayer",
                            "title": "Cluster Feature Layer",
                            "type": "CLUSTER_FEATURE_LAYER",
                            "elevationInfo": {
                                "mode": "relative-to-scene"
                            },
                            "layers": [
                                {
                                    "id": "koeln",
                                    "url": "https://services.conterra.de/arcgis/rest/services/common/koeln/MapServer",
                                    "sublayers": [
                                        {
                                            "id": "1",
                                            "title": "${map.koeln3.places.title}",
                                            "visible": true,
                                            "popupTemplate": {
                                                "title": "{NAME}",
                                                "content": "<div>{ADRESSE} {HAUSNR}, {POSTLEITZAHL} {STADTTEIL}</div>"
                                            }
                                        },
                                        {
                                            "id": "2",
                                            "title": "${map.koeln3.playgrounds.title}",
                                            "visible": true,
                                            "popupTemplate": {
                                                "title": "{Spielplatzname}",
                                                "content": "<div>Viertel: {Stadtviertel}, Stadtteil: {Stadtteil}</div>"
                                            }
                                        },
                                        {
                                            "id": "3",
                                            "title": "${map.koeln3.sights.title}",
                                            "visible": true,
                                            "popupTemplate": {
                                                "title": "{NAME}",
                                                "content": "<div>{ADRESSE} {HAUSNR}, {POSTLEITZAHL} {STADTTEIL}</div>"
                                            }
                                        },
                                        {
                                            "id": "5",
                                            "title": "${map.koeln2.schools.title}",
                                            "visible": false,
                                            "popupTemplate": {
                                                "title": "{NAME}",
                                                "content": "<div>{ADRESSE} {HAUSNR}, {POSTLEITZAHL} {STADTTEIL}</div>"
                                            }
                                        },
                                        {
                                            "id": "6",
                                            "title": "${map.koeln2.museums.title}",
                                            "visible": false,
                                            "popupTemplate": {
                                                "title": "{NAME}",
                                                "content": "<div>{ADRESSE} {HAUSNR}, {POSTLEITZAHL} {STADTTEIL}</div>"
                                            }
                                        },
                                        {
                                            "id": "7",
                                            "title": "${map.koeln2.libraries.title}",
                                            "visible": false,
                                            "popupTemplate": {
                                                "title": "{NAME}",
                                                "content": "<div>{ADRESSE} {HAUSNR}, {POSTLEITZAHL} {STADTTEIL}</div>"
                                            }
                                        }
                                    ]
                                }
                            ],
                            "visible": true,
                            "minScale": 500000,
                            "maxScale": 0,
                            "listMode": "show",
                            "opacity": 1,
                            "outFields": [
                                "*"
                            ],
                            "options": {
                                "objectIdField": "objectid",
                                "clusterDistance": 100,
                                "spiderfyingDistance": 1,
                                "returnLimit": 1000,
                                "maxClusterScale": 0,
                                "symbolBaseSize": 25,
                                "showClusterGrid": true,
                                "showClusterGridCounts": true,
                                "showClusterGridBackground": false,
                                "showClusterArea": true,
                                "showClusterSize": true,
                                "showClusterPopup": false,
                                "showSpiderfying": true,
                                "zoomOnClusterClick": true,
                                "useDefaultSymbolForFeatures": false,
                                "clusterLabelOffset": -4
                            }
                        },
                        {
                            "id": "koeln1",
                            "title": "${map.koeln1.title}",
                            "type": "GROUP",
                            "visible": true,
                            "layers": [
                                {
                                    "title": "${map.koeln1.districts.title}",
                                    "url": "https://services.arcgis.com/ObdAEOfl1Z5LP2D0/arcgis/rest/services/Köln/FeatureServer/8",
                                    "type": "AGS_FEATURE",
                                    "visible": false,
                                    "opacity": 0.5,
                                    "popupTemplate": {
                                        "title": "{STV_NAME}",
                                        "content": [
                                            {
                                                "type": "text",
                                                "text": "${map.koeln1.districts.text}"
                                            }
                                        ]
                                    }
                                },
                                {
                                    "title": "${map.koeln1.boroughs.title}",
                                    "url": "https://services.arcgis.com/ObdAEOfl1Z5LP2D0/arcgis/rest/services/Köln/FeatureServer/7",
                                    "type": "AGS_FEATURE",
                                    "opacity": 0.5,
                                    "minScale": 80000,
                                    "popupTemplate": {
                                        "title": "{NAME}",
                                        "content": [
                                            {
                                                "type": "text",
                                                "text": "${map.koeln1.boroughs.text}"
                                            },
                                            {
                                                "type": "fields",
                                                "fieldInfos": [
                                                    {
                                                        "fieldName": "expression/area",
                                                        "format": {
                                                            "places": 2,
                                                            "digitSeparator": true
                                                        }
                                                    },
                                                    {
                                                        "fieldName": "expression/area-rel",
                                                        "format": {
                                                            "places": 2,
                                                            "digitSeparator": true
                                                        }
                                                    }
                                                ]
                                            }
                                        ],
                                        "expressionInfos": [
                                            {
                                                "name": "area",
                                                "title": "${common.area}",
                                                "expression": "$feature.FLAECHE / 10000"
                                            },
                                            {
                                                "name": "area-rel",
                                                "title": "${common.totalArea}",
                                                "expression": "Round(($feature.FLAECHE / 405020000)*100,2)"
                                            }
                                        ]
                                    }
                                },
                                {
                                    "title": "${map.koeln1.precints.title}",
                                    "url": "https://services.arcgis.com/ObdAEOfl1Z5LP2D0/arcgis/rest/services/Köln/FeatureServer/6",
                                    "type": "AGS_FEATURE",
                                    "opacity": 0.5,
                                    "maxScale": 80000,
                                    "popupTemplate": {
                                        "title": "{NAME}",
                                        "content": [
                                            {
                                                "type": "text",
                                                "text": "${map.koeln1.precints.text}"
                                            },
                                            {
                                                "type": "fields",
                                                "fieldInfos": [
                                                    {
                                                        "fieldName": "expression/area",
                                                        "format": {
                                                            "places": 2,
                                                            "digitSeparator": true
                                                        }
                                                    },
                                                    {
                                                        "fieldName": "expression/area-rel",
                                                        "format": {
                                                            "places": 2,
                                                            "digitSeparator": true
                                                        }
                                                    }
                                                ]
                                            }
                                        ],
                                        "expressionInfos": [
                                            {
                                                "name": "area",
                                                "title": "${common.area}",
                                                "expression": "$feature.FLAECHE / 10000"
                                            },
                                            {
                                                "name": "area-rel",
                                                "title": "${common.totalArea}",
                                                "expression": "Round(($feature.FLAECHE / 405020000)*100,2)"
                                            }
                                        ]
                                    }
                                }
                            ]
                        },
                        {
                            "id": "koeln2",
                            "title": "${map.koeln2.title}",
                            "description": "${map.koeln2.description}",
                            "type": "GROUP",
                            "layers": [
                                {
                                    "title": "${map.koeln2.libraries.title}",
                                    "url": "https://services.arcgis.com/ObdAEOfl1Z5LP2D0/arcgis/rest/services/Köln/FeatureServer/1",
                                    "type": "AGS_FEATURE",
                                    "visible": true,
                                    "popupTemplate": {
                                        "title": "{NAME_LANG}",
                                        "content": [
                                            {
                                                "type": "fields",
                                                "fieldInfos": [
                                                    {
                                                        "fieldName": "NAME_LANG",
                                                        "label": "${common.name}"
                                                    },
                                                    {
                                                        "fieldName": "ADRESSE",
                                                        "label": "${common.address}"
                                                    },
                                                    {
                                                        "fieldName": "PLZ",
                                                        "label": "${common.zip}"
                                                    }
                                                ]
                                            }
                                        ]
                                    }
                                },
                                {
                                    "title": "${map.koeln2.museums.title}",
                                    "url": "https://services.arcgis.com/ObdAEOfl1Z5LP2D0/arcgis/rest/services/Köln/FeatureServer/2",
                                    "type": "AGS_FEATURE",
                                    "visible": true,
                                    "popupTemplate": {
                                        "title": "Museum",
                                        "content": [
                                            {
                                                "type": "text",
                                                "text": "${map.koeln2.museums.text}"
                                            },
                                            {
                                                "type": "fields",
                                                "fieldInfos": [
                                                    {
                                                        "fieldName": "ADRESSE",
                                                        "label": "${common.address}"
                                                    },
                                                    {
                                                        "fieldName": "STADTBEZIR",
                                                        "label": "${common.precint}"
                                                    },
                                                    {
                                                        "fieldName": "HYPERLINK",
                                                        "label": "${common.furtherinfo}"
                                                    }
                                                ]
                                            }
                                        ]
                                    }
                                },
                                {
                                    "id": "schools",
                                    "title": "${map.koeln2.schools.title}",
                                    "url": "https://services.arcgis.com/ObdAEOfl1Z5LP2D0/arcgis/rest/services/Köln/FeatureServer/3",
                                    "type": "AGS_FEATURE",
                                    "visible": false,
                                    "popupTemplate": {
                                        "title": "{NAME}",
                                        "content": [
                                            {
                                                "type": "fields",
                                                "fieldInfos": [
                                                    {
                                                        "fieldName": "SCHULNAME",
                                                        "label": "${common.name}"
                                                    },
                                                    {
                                                        "fieldName": "ADRESSE",
                                                        "label": "${common.address}"
                                                    },
                                                    {
                                                        "fieldName": "SCHULART",
                                                        "label": "${common.type}"
                                                    },
                                                    {
                                                        "fieldName": "TRAEGER",
                                                        "label": "${common.provider}"
                                                    }
                                                ]
                                            }
                                        ]
                                    }
                                }
                            ]
                        },
                        {
                            "id": "koeln3",
                            "title": "${map.koeln3.title}",
                            "type": "GROUP",
                            "visible": true,
                            "layers": [
                                {
                                    "title": "${map.koeln3.sights.title}",
                                    "url": "https://services.arcgis.com/ObdAEOfl1Z5LP2D0/arcgis/rest/services/Köln/FeatureServer/4",
                                    "type": "AGS_FEATURE",
                                    "popupTemplate": {
                                        "title": "${map.koeln3.sights.titleSingle}",
                                        "content": [
                                            {
                                                "type": "text",
                                                "text": "${map.koeln3.sights.text}"
                                            },
                                            {
                                                "type": "fields",
                                                "fieldInfos": [
                                                    {
                                                        "fieldName": "ADRESSE",
                                                        "label": "${common.address}"
                                                    },
                                                    {
                                                        "fieldName": "STADTBEZIRK",
                                                        "label": "${common.precint}"
                                                    },
                                                    {
                                                        "fieldName": "HYPERLINK",
                                                        "label": "${common.furtherinfo}"
                                                    }
                                                ]
                                            }
                                        ]
                                    }
                                },
                                {
                                    "title": "${map.koeln3.playgrounds.title}",
                                    "url": "https://services.arcgis.com/ObdAEOfl1Z5LP2D0/arcgis/rest/services/Köln/FeatureServer/5",
                                    "type": "AGS_FEATURE",
                                    "visible": false,
                                    "popupTemplate": {
                                        "title": "{Typ}",
                                        "content": [
                                            {
                                                "type": "text",
                                                "text": "${map.koeln3.playgrounds.text}"
                                            }
                                        ]
                                    }
                                },
                                {
                                    "title": "${map.koeln3.places.title}",
                                    "url": "https://services.arcgis.com/ObdAEOfl1Z5LP2D0/arcgis/rest/services/Köln/FeatureServer/9",
                                    "type": "AGS_FEATURE",
                                    "popupTemplate": {
                                        "title": "${map.koeln3.places.titleSingle}",
                                        "content": [
                                            {
                                                "type": "text",
                                                "text": "${map.koeln3.places.text}"
                                            },
                                            {
                                                "type": "fields",
                                                "fieldInfos": [
                                                    {
                                                        "fieldName": "ADRESSE",
                                                        "label": "${common.address}"
                                                    },
                                                    {
                                                        "fieldName": "STADTVIERT",
                                                        "label": "${common.district}"
                                                    },
                                                    {
                                                        "fieldName": "HYPERLINK",
                                                        "label": "${common.furtherinfo}"
                                                    }
                                                ]
                                            }
                                        ]
                                    }
                                }
                            ]
                        }
                    ]
                },
                "view": {
                    "viewmode": "2D",
                    "center": {
                        "x": 774853,
                        "y": 6610677,
                        "spatialReference": 3857
                    },
                    "scale": 140000
                }
            }
        },
        "toc": {
            "Config": {
                "showBasemaps": true,
                "showLayerLegend": true
            }
        },
        "toolset": {
            "ToolsetManager": {
                "toolsets": [
                    {
                        "id": "mapview_tools",
                        "tools": [
                            "locateMeTool",
                            "zoomInTool",
                            "zoomOutTool",
                            "compassTool",
                            "restoreInitialViewTool",
                            "viewmodeSwitcherTool"
                        ],
                        "registerWidget": {
                            "widgetRole": "mapview_tools"
                        },
                        "container": "ignore",
                        "windowType": "container",
                        "cssClass": "muted",
                        "tooltipPositions": [
                            "before",
                            "above",
                            "below",
                            "after"
                        ]
                    },
                    {
                        "id": "drawer_left",
                        "title": "${tools.drawerLeft}",
                        "cssClass": "ct-main-app-menu",
                        "tools": [
                            "selection-ui-tool",
                            "sketchToggleTool",
                            "measuring_tools",
                            "printingToggleTool"
                        ],
                        "registerWidget": {
                            "widgetRole": "drawer_button"
                        },
                        "container": "ignore",
                        "windowType": "drawer_left",
                        "tooltipPositions": [
                            "after",
                            "above",
                            "below",
                            "before"
                        ]
                    },
                    {
                        "id": "measuring_tools",
                        "title": "${tools.measuring}",
                        "iconClass": "icon-editor-table",
                        "tools": [
                            "distanceMeasurement2DToggleTool",
                            "areaMeasurement2DToggleTool"
                        ],
                        "container": "ignore",
                        "registerTool": true,
                        "windowType": "menuitem",
                        "tooltipPositions": [
                            "after"
                        ]
                    }
                ]
            }
        },
        "dn_imprintprivacy": {
            "ToolHandler": {
                "imprintUrls": [
                    {
                        "locale": "de",
                        "url": "https://www.conterra.de/impressum"
                    },
                    {
                        "locale": "en",
                        "url": "https://www.con-terra.com/imprint"
                    }
                ],
                "privacyUrls": [
                    {
                        "locale": "de",
                        "url": "https://www.conterra.de/datenschutzhinweise"
                    },
                    {
                        "locale": "en",
                        "url": "https://www.con-terra.com/privacy-statements"
                    }
                ],
                "toolsetDefinition": {
                    "id": "imprintprivacyToolset",
                    "tools": [
                        "imprintTool",
                        "privacyStatementsTool"
                    ],
                    "registerWidget": {
                        "widgetRole": "footer_right"
                    },
                    "container": "ignore",
                    "windowType": "container",
                    "cssClass": "showToolLabels"
                }
            }
        }
    }
}
